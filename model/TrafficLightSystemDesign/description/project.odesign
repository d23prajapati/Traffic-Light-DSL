<?xml version="1.0" encoding="UTF-8"?>
<description:Group xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:description="http://www.eclipse.org/sirius/description/1.1.0" xmlns:description_1="http://www.eclipse.org/sirius/diagram/description/1.1.0" xmlns:properties="http://www.eclipse.org/sirius/properties/1.0.0" xmlns:properties-ext-widgets-reference="http://www.eclipse.org/sirius/properties/1.0.0/ext/widgets/reference" xmlns:style="http://www.eclipse.org/sirius/diagram/description/style/1.1.0" xmlns:tool="http://www.eclipse.org/sirius/diagram/description/tool/1.1.0" xmlns:tool_1="http://www.eclipse.org/sirius/description/tool/1.1.0" xmlns:validation="http://www.eclipse.org/sirius/description/validation/1.1.0" name="project" version="12.0.0.2017041100">
  <ownedViewpoints name="TrafficLightSystemViewPoint">
    <validationSet name="ModelCheck">
      <ownedRules xsi:type="validation:SemanticValidationRule" name="CheckWire" level="ERROR" message="There is a short circuit" targetClass="trafficLightSystem::Connection">
        <audits auditExpression="aql: if (self.src.type=trafficLightSystem::PinType::GND) or (self.target.type=trafficLightSystem::PinType::GND) then not((self.src.type=trafficLightSystem::PinType::VCC) or (self.target.type=trafficLightSystem::PinType::VCC)) else true endif"/>
      </ownedRules>
    </validationSet>
    <ownedRepresentations xsi:type="description_1:DiagramDescription" name="SystemStructureDiagram" domainClass="trafficLightSystem::SystemStructure" enablePopupBars="true">
      <metamodel href="http://www.example.org/trafficLightSystem#/"/>
      <defaultLayer name="Default">
        <nodeMappings name="BoardNode" semanticCandidatesExpression="aql:self.board" domainClass="trafficLightSystem::Board">
          <borderedNodeMappings name="PinGroupNode" semanticCandidatesExpression="aql:self.pinGroups" domainClass="trafficLightSystem::PinGroup">
            <borderedNodeMappings name="PinNode" semanticCandidatesExpression="aql:self.pins" domainClass="trafficLightSystem::Pin">
              <style xsi:type="style:DotDescription" labelSize="12" showIcon="false" labelExpression="aql:''" labelPosition="node" resizeKind="NSEW">
                <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
                <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
                <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='purple']"/>
              </style>
              <conditionnalStyles predicateExpression="aql:self.type=trafficLightSystem::PinType::GND">
                <style xsi:type="style:DotDescription" labelSize="12" showIcon="false" labelPosition="node" resizeKind="NSEW">
                  <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
                  <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
                  <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
                </style>
              </conditionnalStyles>
            </borderedNodeMappings>
            <style xsi:type="style:SquareDescription" labelSize="12" showIcon="false" labelExpression="aql:''" resizeKind="NSEW">
              <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='chocolate']"/>
            </style>
          </borderedNodeMappings>
          <style xsi:type="style:WorkspaceImageDescription" labelSize="12" showIcon="false" sizeComputationExpression="40" labelPosition="node" resizeKind="NSEW" workspacePath="/TrafficLightSystem.project.design/arduinoImages/images/arduino.png">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
          </style>
        </nodeMappings>
        <nodeMappings name="TrafficLightModuleNode" semanticCandidatesExpression="aql:self.components" domainClass="trafficLightSystem::TrafficLightModule">
          <borderedNodeMappings name="RedPinNode" semanticCandidatesExpression="aql:self.redPin" domainClass="trafficLightSystem::Pin">
            <style xsi:type="style:DotDescription" labelSize="12" showIcon="false" labelExpression="aql:''" labelPosition="node" resizeKind="NSEW">
              <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='red']"/>
            </style>
            <conditionnalStyles predicateExpression="aql:self.type=trafficLightSystem::PinType::GND">
              <style xsi:type="style:DotDescription" labelSize="12" showIcon="false" labelPosition="node" resizeKind="NSEW">
                <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
                <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
                <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              </style>
            </conditionnalStyles>
          </borderedNodeMappings>
          <borderedNodeMappings name="YellowPinNode" semanticCandidatesExpression="aql:self.yellowPin" domainClass="trafficLightSystem::Pin">
            <style xsi:type="style:DotDescription" labelSize="12" showIcon="false" labelExpression="aql:''" labelPosition="node" resizeKind="NSEW">
              <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='yellow']"/>
            </style>
            <conditionnalStyles predicateExpression="aql:self.type=trafficLightSystem::PinType::GND">
              <style xsi:type="style:DotDescription" labelSize="12" showIcon="false" labelPosition="node" resizeKind="NSEW">
                <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
                <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
                <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              </style>
            </conditionnalStyles>
          </borderedNodeMappings>
          <borderedNodeMappings name="GreenPinNode" semanticCandidatesExpression="aql:self.greenPin" domainClass="trafficLightSystem::Pin">
            <style xsi:type="style:DotDescription" labelSize="12" showIcon="false" labelExpression="aql:''" labelPosition="node" resizeKind="NSEW">
              <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='green']"/>
            </style>
            <conditionnalStyles predicateExpression="aql:self.type=trafficLightSystem::PinType::GND">
              <style xsi:type="style:DotDescription" labelSize="12" showIcon="false" labelPosition="node" resizeKind="NSEW">
                <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
                <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
                <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              </style>
            </conditionnalStyles>
          </borderedNodeMappings>
          <style xsi:type="style:WorkspaceImageDescription" labelSize="12" showIcon="false" labelExpression="aql:''" iconPath="/TrafficLightSystem.project.design/arduinoImages/images/svg/TrafficLight_RED.svg" sizeComputationExpression="15" labelPosition="node" resizeKind="NSEW" workspacePath="/TrafficLightSystem.project.design/arduinoImages/images/svg/vertical-traffic-light-modlue.svg">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
          </style>
        </nodeMappings>
        <nodeMappings name="LEDNode" semanticCandidatesExpression="aql:self.components" domainClass="trafficLightSystem::LED" reusedBorderedNodeMappings="//@ownedViewpoints[name='TrafficLightSystemViewPoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@nodeMappings[name='BoardNode']/@borderedNodeMappings[name='PinGroupNode']">
          <style xsi:type="style:WorkspaceImageDescription" labelSize="12" showIcon="false" labelExpression="aql:''" sizeComputationExpression="10" labelPosition="node" resizeKind="NSEW" workspacePath="/TrafficLightSystem.project.design/arduinoImages/images/svg/led-green.svg">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
          </style>
          <conditionnalStyles predicateExpression="aql:self.color=trafficLightSystem::LightColor::RED">
            <style xsi:type="style:WorkspaceImageDescription" labelSize="12" showIcon="false" labelExpression="aql:''" sizeComputationExpression="10" labelPosition="node" resizeKind="NSEW" workspacePath="/TrafficLightSystem.project.design/arduinoImages/images/svg/led-red.svg">
              <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </style>
          </conditionnalStyles>
        </nodeMappings>
        <nodeMappings name="ButtonNode" semanticCandidatesExpression="aql:self.components" domainClass="trafficLightSystem::Button" reusedBorderedNodeMappings="//@ownedViewpoints[name='TrafficLightSystemViewPoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@nodeMappings[name='BoardNode']/@borderedNodeMappings[name='PinGroupNode']">
          <style xsi:type="style:WorkspaceImageDescription" labelSize="12" showIcon="false" labelExpression="aql:''" sizeComputationExpression="10" labelPosition="node" resizeKind="NSEW" workspacePath="/TrafficLightSystem.project.design/arduinoImages/images/svg/Push_button.svg">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
          </style>
        </nodeMappings>
        <edgeMappings name="WireEdge" semanticCandidatesExpression="aql:self.connections" sourceMapping="//@ownedViewpoints[name='TrafficLightSystemViewPoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@nodeMappings[name='TrafficLightModuleNode']/@borderedNodeMappings[name='RedPinNode'] //@ownedViewpoints[name='TrafficLightSystemViewPoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@nodeMappings[name='BoardNode']/@borderedNodeMappings[name='PinGroupNode']/@borderedNodeMappings[name='PinNode'] //@ownedViewpoints[name='TrafficLightSystemViewPoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@nodeMappings[name='TrafficLightModuleNode']/@borderedNodeMappings[name='GreenPinNode'] //@ownedViewpoints[name='TrafficLightSystemViewPoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@nodeMappings[name='TrafficLightModuleNode']/@borderedNodeMappings[name='YellowPinNode']" targetMapping="//@ownedViewpoints[name='TrafficLightSystemViewPoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@nodeMappings[name='TrafficLightModuleNode']/@borderedNodeMappings[name='RedPinNode'] //@ownedViewpoints[name='TrafficLightSystemViewPoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@nodeMappings[name='BoardNode']/@borderedNodeMappings[name='PinGroupNode']/@borderedNodeMappings[name='PinNode'] //@ownedViewpoints[name='TrafficLightSystemViewPoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@nodeMappings[name='TrafficLightModuleNode']/@borderedNodeMappings[name='GreenPinNode'] //@ownedViewpoints[name='TrafficLightSystemViewPoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@nodeMappings[name='TrafficLightModuleNode']/@borderedNodeMappings[name='YellowPinNode']" targetFinderExpression="aql:self.target" sourceFinderExpression="aql:self.src" domainClass="trafficLightSystem::Connection" useDomainElement="true">
          <style sizeComputationExpression="2">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <centerLabelStyleDescription labelSize="12" showIcon="false">
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
        <toolSections name="TrafficLightSystemTools">
          <ownedTools xsi:type="tool:NodeCreationDescription" name="Create TrafficLight Module" nodeMappings="//@ownedViewpoints[name='TrafficLightSystemViewPoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@nodeMappings[name='TrafficLightModuleNode']" iconPath="/TrafficLightSystem.project.design/arduinoImages/images/svg/traffic-light-icon.jpg">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:container">
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="trafficLightSystem::TrafficLightModule" referenceName="components" variableName="TrafficLightModuleInstance">
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="new Traffic Light Module"/>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:NodeCreationDescription" name="Create LED" nodeMappings="//@ownedViewpoints[name='TrafficLightSystemViewPoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@nodeMappings[name='LEDNode']" iconPath="/TrafficLightSystem.project.design/arduinoImages/images/svg/LED_ICON.png">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:container">
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="trafficLightSystem::LED" referenceName="components" variableName="LEDInstance">
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="new LED"/>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:NodeCreationDescription" name="Create Button" nodeMappings="//@ownedViewpoints[name='TrafficLightSystemViewPoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@nodeMappings[name='ButtonNode']" iconPath="/TrafficLightSystem.project.design/arduinoImages/images/svg/BUTTON_ICON.png">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:container">
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="trafficLightSystem::Button" referenceName="components" variableName="ButtonInstance">
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="new Button"/>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:NodeCreationDescription" name="Create Pin" precondition="aql:container.oclIsKindOf(trafficLightSystem::PinGroup) or container.oclIsKindOf(trafficLightSystem::Board) or container.oclIsKindOf(trafficLightSystem::LED)  or container.oclIsKindOf(trafficLightSystem::TrafficLightModule) or container.oclIsKindOf(trafficLightSystem::Button)" nodeMappings="//@ownedViewpoints[name='TrafficLightSystemViewPoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@nodeMappings[name='TrafficLightModuleNode']/@borderedNodeMappings[name='RedPinNode'] //@ownedViewpoints[name='TrafficLightSystemViewPoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@nodeMappings[name='BoardNode']/@borderedNodeMappings[name='PinGroupNode']/@borderedNodeMappings[name='PinNode']" extraMappings="//@ownedViewpoints[name='TrafficLightSystemViewPoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@nodeMappings[name='BoardNode'] //@ownedViewpoints[name='TrafficLightSystemViewPoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@nodeMappings[name='ButtonNode'] //@ownedViewpoints[name='TrafficLightSystemViewPoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@nodeMappings[name='LEDNode'] //@ownedViewpoints[name='TrafficLightSystemViewPoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@nodeMappings[name='TrafficLightModuleNode']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:Switch">
                <cases conditionExpression="aql:container.oclIsKindOf(trafficLightSystem::PinGroup)">
                  <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="aql:container.oclAsType(trafficLightSystem::PinGroup)">
                    <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:container">
                      <subModelOperations xsi:type="tool_1:CreateInstance" typeName="trafficLightSystem::Pin" referenceName="pins" variableName="PinInstance">
                        <subModelOperations xsi:type="tool_1:SetValue" featureName="pinNr" valueExpression="0"/>
                        <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="Name"/>
                        <subModelOperations xsi:type="tool_1:SetValue" featureName="type" valueExpression="Type"/>
                      </subModelOperations>
                    </subModelOperations>
                  </subModelOperations>
                </cases>
                <cases conditionExpression="aql:container.oclIsKindOf(trafficLightSystem::Board) or container.oclIsKindOf(trafficLightSystem::LED)  or container.oclIsKindOf(trafficLightSystem::Button)">
                  <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:container">
                    <subModelOperations xsi:type="tool_1:CreateInstance" typeName="trafficLightSystem::PinGroup" referenceName="pinGroups" variableName="groupInstance">
                      <subModelOperations xsi:type="tool_1:CreateInstance" typeName="trafficLightSystem::Pin" referenceName="pins" variableName="pinInstance">
                        <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:pinInstance">
                          <subModelOperations xsi:type="tool_1:SetValue" featureName="pinNr" valueExpression="0"/>
                        </subModelOperations>
                      </subModelOperations>
                    </subModelOperations>
                  </subModelOperations>
                </cases>
                <cases conditionExpression="aql:container.oclIsKindOf(trafficLightSystem::TrafficLightModule)">
                  <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:container">
                    <subModelOperations xsi:type="tool_1:CreateInstance" typeName="trafficLightSystem::Pin" referenceName="redPin" variableName="redPinInstance">
                      <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:redPinInstance">
                        <subModelOperations xsi:type="tool_1:SetValue" featureName="pinNr" valueExpression="0"/>
                      </subModelOperations>
                    </subModelOperations>
                    <subModelOperations xsi:type="tool_1:CreateInstance" typeName="trafficLightSystem::Pin" referenceName="yellowPin" variableName="yellowPinInstance">
                      <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:yellowPinInstance">
                        <subModelOperations xsi:type="tool_1:SetValue" featureName="pinNr" valueExpression="0"/>
                      </subModelOperations>
                    </subModelOperations>
                    <subModelOperations xsi:type="tool_1:CreateInstance" typeName="trafficLightSystem::Pin" referenceName="greenPin" variableName="greenPinInstance">
                      <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:greenPinInstance">
                        <subModelOperations xsi:type="tool_1:SetValue" featureName="pinNr" valueExpression="0"/>
                      </subModelOperations>
                    </subModelOperations>
                  </subModelOperations>
                </cases>
                <default>
                  <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:container">
                    <subModelOperations xsi:type="tool_1:CreateInstance" typeName="trafficLightSystem::Pin" referenceName="pins" variableName="PinInstance"/>
                  </subModelOperations>
                </default>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:EdgeCreationDescription" name="WireEdge" edgeMappings="//@ownedViewpoints[name='TrafficLightSystemViewPoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@edgeMappings[name='WireEdge']">
            <sourceVariable name="source"/>
            <targetVariable name="target"/>
            <sourceViewVariable name="sourceView"/>
            <targetViewVariable name="targetView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="aql:source.ancestors()->filter(trafficLightSystem::SystemStructure)->first()">
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="trafficLightSystem::Connection" referenceName="connections">
                  <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:instance">
                    <subModelOperations xsi:type="tool_1:SetValue" featureName="src" valueExpression="var:source"/>
                    <subModelOperations xsi:type="tool_1:SetValue" featureName="target" valueExpression="var:target"/>
                  </subModelOperations>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool_1:ToolDescription" name="PrintComponentName">
            <element name="element"/>
            <elementView name="elementView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="aql:self.oclAsType(trafficLightSystem::Component).printComponentName()"/>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:NodeCreationDescription" name="Create Board" nodeMappings="//@ownedViewpoints[name='TrafficLightSystemViewPoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@nodeMappings[name='BoardNode']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:container">
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="trafficLightSystem::Board" referenceName="board">
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="new Board"/>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
        </toolSections>
      </defaultLayer>
    </ownedRepresentations>
    <ownedRepresentations xsi:type="description_1:DiagramDescription" name="SystemBehaviorDiagram" domainClass="trafficLightSystem::SystemBehavior" enablePopupBars="true">
      <metamodel href="http://www.example.org/trafficLightSystem#/"/>
      <defaultLayer name="Default">
        <edgeMappings name="PhaseTransition" sourceMapping="//@ownedViewpoints[name='TrafficLightSystemViewPoint']/@ownedRepresentations[name='SystemBehaviorDiagram']/@defaultLayer/@containerMappings[name='JunctionController']/@subContainerMappings[name='PhaseContainer']" targetMapping="//@ownedViewpoints[name='TrafficLightSystemViewPoint']/@ownedRepresentations[name='SystemBehaviorDiagram']/@defaultLayer/@containerMappings[name='JunctionController']/@subContainerMappings[name='PhaseContainer']" targetFinderExpression="aql:if self.eContainer().phases->last() = self then self.eContainer().phases->first() else self.eContainer().phases->at(self.eContainer().phases->indexOf(self) + 1) endif">
          <style sizeComputationExpression="2">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <centerLabelStyleDescription labelSize="12">
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
        <containerMappings name="JunctionController" semanticCandidatesExpression="aql:self.junctionController" domainClass="trafficLightSystem::JunctionController">
          <subContainerMappings name="PhaseContainer" semanticCandidatesExpression="aql:self.phases" domainClass="trafficLightSystem::Phase">
            <subNodeMappings name="TrafficAssignmentNode" semanticCandidatesExpression="aql:self.trafficLightAssignments" domainClass="trafficLightSystem::TrafficLightAssignment">
              <style xsi:type="style:SquareDescription" labelSize="12" labelExpression="aql:''" labelPosition="node" resizeKind="NSEW">
                <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
                <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
                <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='gray']"/>
              </style>
              <conditionnalStyles predicateExpression="aql:self.color = trafficLightSystem::LightColor::RED">
                <style xsi:type="style:WorkspaceImageDescription" labelSize="12" showIcon="false" labelExpression="aql:self.trafficLightModule.name" sizeComputationExpression="10" resizeKind="NSEW" workspacePath="/TrafficLightSystem.project.design/arduinoImages/images/svg/TrafficLight_RED.svg">
                  <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
                  <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
                </style>
              </conditionnalStyles>
              <conditionnalStyles predicateExpression="aql:self.color = trafficLightSystem::LightColor::RED_YELLOW">
