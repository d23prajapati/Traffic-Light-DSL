<?xml version="1.0" encoding="UTF-8"?>
<description:Group xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:description="http://www.eclipse.org/sirius/description/1.1.0" xmlns:description_1="http://www.eclipse.org/sirius/diagram/description/1.1.0" xmlns:properties="http://www.eclipse.org/sirius/properties/1.0.0" xmlns:style="http://www.eclipse.org/sirius/diagram/description/style/1.1.0" xmlns:tool="http://www.eclipse.org/sirius/diagram/description/tool/1.1.0" xmlns:tool_1="http://www.eclipse.org/sirius/description/tool/1.1.0" xmlns:validation="http://www.eclipse.org/sirius/description/validation/1.1.0" name="project" version="12.0.0.2017041100">
  <ownedViewpoints name="TrafficLightSystemViewPoint">
    <validationSet name="ModelCheck">
      <ownedRules xsi:type="validation:SemanticValidationRule" name="CheckWire" level="ERROR" message="There is a short circuit" targetClass="trafficLightSystem::Connection">
        <audits auditExpression="aql: if (self.src.type=trafficLightSystem::PinType::GND) or (self.target.type=trafficLightSystem::PinType::GND) then not((self.src.type=trafficLightSystem::PinType::VCC) or (self.target.type=trafficLightSystem::PinType::VCC)) else true endif"/>
      </ownedRules>
    </validationSet>
    <ownedRepresentations xsi:type="description_1:DiagramDescription" name="SystemStructureDiagram" domainClass="trafficLightSystem::SystemStructure" enablePopupBars="true">
      <metamodel href="http://www.example.org/trafficLightSystem#/"/>
      <defaultLayer name="Default">
        <nodeMappings name="BoardNode" semanticCandidatesExpression="aql:self.board" domainClass="trafficLightSystem::Board" reusedBorderedNodeMappings="//@ownedViewpoints[name='TrafficLightSystemViewPoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@nodeMappings[name='TrafficLightModuleNode']/@borderedNodeMappings[name='PinGroupNode']">
          <style xsi:type="style:WorkspaceImageDescription" labelSize="12" sizeComputationExpression="-1" labelPosition="node" resizeKind="NSEW" workspacePath="/TrafficLightSystem.project.design/arduinoImages/images/arduino.png">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
          </style>
        </nodeMappings>
        <nodeMappings name="TrafficLightModuleNode" semanticCandidatesExpression="aql:self.components" domainClass="trafficLightSystem::TrafficLightModule">
          <borderedNodeMappings name="PinGroupNode" semanticCandidatesExpression="aql:self.pinGroups" domainClass="trafficLightSystem::PinGroup">
            <borderedNodeMappings name="PinNode" semanticCandidatesExpression="aql:self.pins" domainClass="trafficLightSystem::Pin">
              <style xsi:type="style:DotDescription" labelSize="12" labelExpression="aql:''" labelPosition="node" resizeKind="NSEW">
                <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
                <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
                <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='purple']"/>
              </style>
              <conditionnalStyles predicateExpression="aql:self.type=trafficLightSystem::PinType::GND">
                <style xsi:type="style:DotDescription" labelSize="12" labelPosition="node" resizeKind="NSEW">
                  <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
                  <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
                  <backgroundColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
                </style>
              </conditionnalStyles>
            </borderedNodeMappings>
            <style xsi:type="style:SquareDescription" labelSize="12" labelExpression="aql:self.name" resizeKind="NSEW">
              <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
              <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='chocolate']"/>
            </style>
          </borderedNodeMappings>
          <style xsi:type="style:WorkspaceImageDescription" labelSize="12" showIcon="false" labelExpression="aql:''" sizeComputationExpression="-1" labelPosition="node" resizeKind="NSEW" workspacePath="/TrafficLightSystem.project.design/arduinoImages/images/svg/vertical-traffic-light-modlue.svg">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
          </style>
        </nodeMappings>
        <nodeMappings name="LEDNode" semanticCandidatesExpression="aql:self.components" domainClass="trafficLightSystem::LED" reusedBorderedNodeMappings="//@ownedViewpoints[name='TrafficLightSystemViewPoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@nodeMappings[name='TrafficLightModuleNode']/@borderedNodeMappings[name='PinGroupNode']">
          <style xsi:type="style:WorkspaceImageDescription" labelSize="12" sizeComputationExpression="-1" labelPosition="node" resizeKind="NSEW" workspacePath="/TrafficLightSystem.project.design/arduinoImages/images/svg/led_unit.svg">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
          </style>
        </nodeMappings>
        <nodeMappings name="ButtonNode" semanticCandidatesExpression="aql:self.components" domainClass="trafficLightSystem::Button" reusedBorderedNodeMappings="//@ownedViewpoints[name='TrafficLightSystemViewPoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@nodeMappings[name='TrafficLightModuleNode']/@borderedNodeMappings[name='PinGroupNode']">
          <style xsi:type="style:WorkspaceImageDescription" labelSize="12" sizeComputationExpression="-1" labelPosition="node" resizeKind="NSEW" workspacePath="/TrafficLightSystem.project.design/arduinoImages/images/svg/Push_button.svg">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
          </style>
        </nodeMappings>
        <edgeMappings name="WireEdge" semanticCandidatesExpression="aql:self.connections" sourceMapping="//@ownedViewpoints[name='TrafficLightSystemViewPoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@nodeMappings[name='TrafficLightModuleNode']/@borderedNodeMappings[name='PinGroupNode']/@borderedNodeMappings[name='PinNode']" targetMapping="//@ownedViewpoints[name='TrafficLightSystemViewPoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@nodeMappings[name='TrafficLightModuleNode']/@borderedNodeMappings[name='PinGroupNode']/@borderedNodeMappings[name='PinNode']" targetFinderExpression="aql:self.target" sourceFinderExpression="aql:self.src" domainClass="trafficLightSystem::Connection" useDomainElement="true">
          <style sizeComputationExpression="2">
            <strokeColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <centerLabelStyleDescription labelSize="12">
              <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            </centerLabelStyleDescription>
          </style>
        </edgeMappings>
        <toolSections name="TrafficLightSystemTools">
          <ownedTools xsi:type="tool:NodeCreationDescription" name="Create TrafficLight Module" nodeMappings="//@ownedViewpoints[name='TrafficLightSystemViewPoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@nodeMappings[name='TrafficLightModuleNode']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:container">
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="trafficLightSystem::TrafficLightModule" referenceName="components" variableName="TrafficLightModuleInstance">
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="new Traffic Light Module"/>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:NodeCreationDescription" name="Create LED" nodeMappings="//@ownedViewpoints[name='TrafficLightSystemViewPoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@nodeMappings[name='LEDNode']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:container">
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="trafficLightSystem::LED" referenceName="components" variableName="LEDInstance">
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="new LED"/>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:NodeCreationDescription" name="Create Button" nodeMappings="//@ownedViewpoints[name='TrafficLightSystemViewPoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@nodeMappings[name='ButtonNode']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:container">
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="trafficLightSystem::Button" referenceName="components" variableName="ButtonInstance">
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="new Button"/>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:NodeCreationDescription" name="Create Pin" precondition="aql:container.oclIsKindOf(trafficLightSystem::PinGroup) or container.oclIsKindOf(trafficLightSystem::Board) or container.oclIsKindOf(trafficLightSystem::LED) or container.oclIsKindOf(trafficLightSystem::TrafficLightModule) or container.oclIsKindOf(trafficLightSystem::Button)" nodeMappings="//@ownedViewpoints[name='TrafficLightSystemViewPoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@nodeMappings[name='TrafficLightModuleNode']/@borderedNodeMappings[name='PinGroupNode']/@borderedNodeMappings[name='PinNode']" extraMappings="//@ownedViewpoints[name='TrafficLightSystemViewPoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@nodeMappings[name='BoardNode'] //@ownedViewpoints[name='TrafficLightSystemViewPoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@nodeMappings[name='ButtonNode'] //@ownedViewpoints[name='TrafficLightSystemViewPoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@nodeMappings[name='LEDNode'] //@ownedViewpoints[name='TrafficLightSystemViewPoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@nodeMappings[name='TrafficLightModuleNode']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:Switch">
                <cases conditionExpression="aql:container.oclIsKindOf(trafficLightSystem::PinGroup)">
                  <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="aql:container.oclAsType(trafficLightSystem::PinGroup)">
                    <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:container">
                      <subModelOperations xsi:type="tool_1:CreateInstance" typeName="trafficLightSystem::Pin" referenceName="pins" variableName="PinInstance">
                        <subModelOperations xsi:type="tool_1:SetValue" featureName="pinNr" valueExpression="0"/>
                        <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="Name"/>
                        <subModelOperations xsi:type="tool_1:SetValue" featureName="type" valueExpression="Type"/>
                      </subModelOperations>
                    </subModelOperations>
                  </subModelOperations>
                </cases>
                <cases conditionExpression="aql:container.oclIsKindOf(trafficLightSystem::Board) or container.oclIsKindOf(trafficLightSystem::LED) or container.oclIsKindOf(trafficLightSystem::TrafficLightModule) or container.oclIsKindOf(trafficLightSystem::Button)">
                  <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:container">
                    <subModelOperations xsi:type="tool_1:CreateInstance" typeName="trafficLightSystem::PinGroup" referenceName="pinGroups" variableName="groupInstance">
                      <subModelOperations xsi:type="tool_1:CreateInstance" typeName="trafficLightSystem::Pin" referenceName="pins" variableName="pinInstance">
                        <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:pinInstance">
                          <subModelOperations xsi:type="tool_1:SetValue" featureName="pinNr" valueExpression="0"/>
                        </subModelOperations>
                      </subModelOperations>
                    </subModelOperations>
                  </subModelOperations>
                </cases>
                <default>
                  <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:container">
                    <subModelOperations xsi:type="tool_1:CreateInstance" typeName="trafficLightSystem::Pin" referenceName="pins" variableName="PinInstance"/>
                  </subModelOperations>
                </default>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool:EdgeCreationDescription" name="WireEdge" edgeMappings="//@ownedViewpoints[name='TrafficLightSystemViewPoint']/@ownedRepresentations[name='SystemStructureDiagram']/@defaultLayer/@edgeMappings[name='WireEdge']">
            <sourceVariable name="source"/>
            <targetVariable name="target"/>
            <sourceViewVariable name="sourceView"/>
            <targetViewVariable name="targetView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="aql:source.ancestors()->filter(trafficLightSystem::SystemStructure)->first()">
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="trafficLightSystem::Connection" referenceName="connections">
                  <subModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:instance">
                    <subModelOperations xsi:type="tool_1:SetValue" featureName="src" valueExpression="var:source"/>
                    <subModelOperations xsi:type="tool_1:SetValue" featureName="target" valueExpression="var:target"/>
                  </subModelOperations>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
          <ownedTools xsi:type="tool_1:ToolDescription" name="PrintComponentName">
            <element name="element"/>
            <elementView name="elementView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="aql:self.oclAsType(trafficLightSystem::Component).printComponentName()"/>
            </initialOperation>
          </ownedTools>
        </toolSections>
      </defaultLayer>
    </ownedRepresentations>
    <ownedRepresentations xsi:type="description_1:DiagramDescription" name="BehaviorDiagram" domainClass="trafficLightSystem::SystemBehavior" enablePopupBars="true">
      <defaultLayer name="Default">
        <nodeMappings name="BehaviorNode" semanticCandidatesExpression="aql:self.behaviors" domainClass="trafficLightSystem::Behavior">
          <style xsi:type="style:EllipseNodeDescription" labelSize="12" labelPosition="node" resizeKind="NSEW">
            <borderColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <labelColor xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='black']"/>
            <color xsi:type="description:SystemColor" href="environment:/viewpoint#//@systemColors/@entries[name='yellow']"/>
          </style>
        </nodeMappings>
        <toolSections name="BehaviorTool">
          <ownedTools xsi:type="tool:NodeCreationDescription" name="BehaviorNode" nodeMappings="//@ownedViewpoints[name='TrafficLightSystemViewPoint']/@ownedRepresentations[name='BehaviorDiagram']/@defaultLayer/@nodeMappings[name='BehaviorNode']">
            <variable name="container"/>
            <viewVariable name="containerView"/>
            <initialOperation>
              <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:container">
                <subModelOperations xsi:type="tool_1:CreateInstance" typeName="trafficLightSystem::Behavior" referenceName="behaviors">
                  <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="name"/>
                </subModelOperations>
              </firstModelOperations>
            </initialOperation>
          </ownedTools>
        </toolSections>
      </defaultLayer>
    </ownedRepresentations>
    <ownedJavaExtensions qualifiedClassName="TrafficLightSystem.project.design.Services"/>
  </ownedViewpoints>
  <extensions xsi:type="properties:ViewExtensionDescription" name="Properties">
    <categories name="StructureCategory">
      <pages name="TrafficLightModulePage" labelExpression="Traffic Light Module Setup" domainClass="trafficLightSystem::TrafficLightModule" semanticCandidateExpression="var:self" groups="//@extensions.0/@categories.0/@groups.0">
        <validationSet>
          <semanticValidationRules name="NameCheck" level="ERROR" message="Empty name" targetClass="trafficLightSystem::TrafficLightModule">
            <audits auditExpression="aql:self.oclAsType(trafficLightSystem::TrafficLightModule).name.size()>0"/>
          </semanticValidationRules>
        </validationSet>
      </pages>
      <pages name="LEDPage" labelExpression="LED Setup" domainClass="trafficLightSystem::LED" semanticCandidateExpression="var:self" groups="//@extensions.0/@categories.0/@groups.0 //@extensions.0/@categories.0/@groups.2">
        <validationSet>
          <semanticValidationRules name="NameCheck" level="ERROR" message="Empty name" targetClass="trafficLightSystem::LED">
            <audits auditExpression="aql:self.oclAsType(trafficLightSystem::LED).name.size()>0"/>
          </semanticValidationRules>
        </validationSet>
      </pages>
      <pages name="ButtonPage" labelExpression="Button Setup" domainClass="trafficLightSystem::Button" semanticCandidateExpression="var:self" groups="//@extensions.0/@categories.0/@groups.0">
        <validationSet>
          <semanticValidationRules name="NameCheck" level="ERROR" message="Empty name" targetClass="trafficLightSystem::Button">
            <audits auditExpression="aql:self.oclAsType(trafficLightSystem::Button).name.size()>0"/>
          </semanticValidationRules>
        </validationSet>
      </pages>
      <pages name="PinPage" labelExpression="Pin Setup " domainClass="trafficLightSystem::Pin" semanticCandidateExpression="var:self" groups="//@extensions.0/@categories.0/@groups.0 //@extensions.0/@categories.0/@groups.1">
        <validationSet>
          <semanticValidationRules name="NameCheck" level="ERROR" message="Empty name" targetClass="trafficLightSystem::Pin">
            <audits auditExpression="aql:self.oclAsType(trafficLightSystem::PIN).name.size()>0"/>
          </semanticValidationRules>
        </validationSet>
      </pages>
      <pages name="BoardPage" labelExpression="Board Setup" domainClass="trafficLightSystem::Board" semanticCandidateExpression="var:self" groups="//@extensions.0/@categories.0/@groups.0">
        <validationSet>
          <semanticValidationRules name="NameCheck" level="ERROR" message="Empty name" targetClass="trafficLightSystem::Board">
            <audits auditExpression="aql:self.oclAsType(trafficLightSystem::Board).name.size()>0"/>
          </semanticValidationRules>
        </validationSet>
      </pages>
      <groups name="GeneralGroup" labelExpression="General Setup" domainClass="EObject" semanticCandidateExpression="var:self" preconditionExpression="aql:self.oclIsKindOf(trafficLightSystem::Board) or self.oclIsKindOf(trafficLightSystem::TrafficLightModule) or self.oclIsKindOf(trafficLightSystem::LED) or self.oclIsKindOf(trafficLightSystem::Button) or self.oclIsKindOf(trafficLightSystem::Pin)">
        <controls xsi:type="properties:TextDescription" name="NameTextEdit" labelExpression="Name" valueExpression="aql:if self.oclIsKindOf(trafficLightSystem::Board) then self.oclAsType(trafficLightSystem::Board).name else if self.oclIsKindOf(trafficLightSystem::TrafficLightModule) then self.oclAsType(trafficLightSystem::TrafficLightModule).name else if self.oclIsKindOf(trafficLightSystem::LED) then self.oclAsType(trafficLightSystem::LED).name else if self.oclIsKindOf(trafficLightSystem::Button) then self.oclAsType(trafficLightSystem::Button).name else if self.oclIsKindOf(trafficLightSystem::Pin) then self.oclAsType(trafficLightSystem::Pin).name else '' endif endif endif endif endif&#xD;">
          <initialOperation>
            <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:self">
              <subModelOperations xsi:type="tool_1:SetValue" featureName="name" valueExpression="var:newValue"/>
            </firstModelOperations>
          </initialOperation>
        </controls>
      </groups>
      <groups name="PinGroup" labelExpression="Pin Setup" domainClass="trafficLightSystem::Pin" semanticCandidateExpression="var:self" preconditionExpression="aql:self.oclIsKindOf(trafficLightSystem::Pin)">
        <controls xsi:type="properties:TextDescription" name="PinNumberEdit" labelExpression="Pin Number" valueExpression="aql:self.pinNr">
          <initialOperation>
            <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:self">
              <subModelOperations xsi:type="tool_1:SetValue" featureName="pinNr" valueExpression="var:newValue"/>
            </firstModelOperations>
          </initialOperation>
        </controls>
        <controls xsi:type="properties:RadioDescription" name="PinTypeRadio" labelExpression="Pin Type" valueExpression="aql:self.type.toString()" candidatesExpression="aql:trafficLightSystem::PinType.eContents().toString()" candidateDisplayExpression="aql:candidate.toString()">
          <initialOperation>
            <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:self">
              <subModelOperations xsi:type="tool_1:SetValue" featureName="type" valueExpression="var:newValue"/>
            </firstModelOperations>
          </initialOperation>
        </controls>
      </groups>
      <groups name="LEDGroup" labelExpression="LED Setup" domainClass="trafficLightSystem::LED" semanticCandidateExpression="var:self">
        <controls xsi:type="properties:RadioDescription" name="ColorRadio" labelExpression="Color" valueExpression="aql:self.color.toString()" candidatesExpression="aql:trafficLightSystem::LightColor.eContents().toString()" candidateDisplayExpression="aql:candidate.toString()">
          <initialOperation>
            <firstModelOperations xsi:type="tool_1:ChangeContext" browseExpression="var:self">
              <subModelOperations xsi:type="tool_1:SetValue" featureName="color" valueExpression="var:newValue"/>
            </firstModelOperations>
          </initialOperation>
        </controls>
      </groups>
