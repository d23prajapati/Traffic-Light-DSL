[module getPinNumber('http://www.example.org/trafficLightSystem')]


[query public getPinNumber (aTrafficLightModule : TrafficLightModule, pinName : String) : Integer = 

	aTrafficLightModule.pinGroups->collect(pg | pg.pins)->flatten()->select(p | p.name = pinName)->first().pinNr

/]

[query public getPinNumber (aLED : LED, pinName : String) : Integer = 

	aLED.pinGroups->collect(pg | pg.pins)->flatten()->select(p | p.name = pinName)->first().pinNr

/]
