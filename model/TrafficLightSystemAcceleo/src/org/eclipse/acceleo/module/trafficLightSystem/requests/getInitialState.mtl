[comment encoding = UTF-8 /]
[module getInitialState('http://www.example.org/trafficLightSystem')]


[query public getInitialState (aSystemBehavior : SystemBehavior) : String = 
	'STATE_'.concat(
	aSystemBehavior.behaviors
	-> select(b | b.oclIsKindOf(StateMachine))->first().oclAsType(StateMachine).states
	->select(s | s.isInitialState = true)->first()
	.name.toUpper()
	)
/]
